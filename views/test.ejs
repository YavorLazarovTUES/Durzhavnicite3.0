<!DOCTYPE html>
<html>
<head>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
</head>
<body>
    <div>
        <canvas id="myChart"></canvas>
    </div>
    
    <script>
        const ctx = document.getElementById('myChart');

        const stockPrices = <%- JSON.stringify(stock_price_array)  %>;
        console.log(stockPrices)
        console.log(typeof(stockPrices))

        const labels =[];   
        for (let i = 1; i <= 20; i++) {
        labels.push(`day${i}`);
        }
        const data = {
            labels: labels,
            datasets: [{
                label: 'AAPL stock price',
                data:stockPrices,
                fill: true,
                borderColor: 'rgb(0, 0, 192)',
                tension: 0
            }]
        };

        new Chart(ctx, {
            type: 'line',
            data: data
        });
    </script>
</body>
</html>
